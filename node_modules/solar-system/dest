var solarSystemComponent = React.createClass({
  displayName: 'solarSystemComponent',

  render: function render() {
    return React.createElement(
      'h1',
      null,
      'Hello, world!'
    );
  }
});

document.registerReact('solar-system', SolarSystemComponent);
ReactDOM.render(React.createElement(SolarSystemComponent, null), document.getElementsByTagName('solar-system'));
